GOOGLEAPIS=vendor/github.com/grpc-ecosystem/grpc-gateway/third_party/googleapis

ATTRIBUTES="{\
  \"name\":\"daniel ortega\"
}"

CREATE="{\
  \"data\":{\
    \"type\":\"article\",
    \"attributes\":${ATTRIBUTES}
  }
}"

UPDATE="{\
  \"data\":{\
    \"type\":\"article\",
    \"id\":\"${INSTANCE_ID}\",
    \"attributes\":${ATTRIBUTES}
  }
}"

VALIDATE="{\
  \"data\":{\
    \"type\": \"article\",
    \"attributes\":${ATTRIBUTES}
  }
}"

FIND="\
page=1&\
limit=10&\
sortBy=name&\
${QUERY}\
"

# prefix properties with "filter."
QUERY="\
filter.firebaseId=${FIREBASE_ID}\
"
